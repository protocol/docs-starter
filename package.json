{
    "name": "@daas/docs-starter",
    "description": "Docs starter",
    "version": "0.0.1",
    "engines": {
        "node": ">=16.16.0"
    },
    "browserslist": [
        "defaults"
    ],
    "repository": "https://github.com/protocol/docs-starter",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "create": "exec-bin node_modules/.bin/hugo/hugo new",
        "start": "exec-bin node_modules/.bin/hugo/hugo server --bind=0.0.0.0 --disableFastRender",
        "build": "exec-bin node_modules/.bin/hugo/hugo --gc --minify"
    },
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^4.1",
        "@hyas/images": "^0.2.2",
        "bootstrap": "^5.2.0-beta1",
        "clipboard": "^2.0",
        "exec-bin": "^1.0.0",
        "flexsearch": "^0.7.21",
        "highlight.js": "^11.5",
        "hugo-installer": "^3.1",
        "instant.page": "^5.1",
        "katex": "^0.16",
        "lazysizes": "^5.3",
        "node-fetch": "^3.2",
        "shx": "^0.3.4"
    },
    "otherDependencies": {
        "hugo": "0.101.0"
    }
}
